
<div class = "toukou">
<div class = "box10">

<h1>投稿する</h1>
<%= form_for [@blog] do |f| %>
<div>
<%= f.label :題名%>
</div>
<div>
<%= f.text_field :title, placeholder: 'title' %>
</div>
<div>
<%= f.label :内容%>
</div>
<div>
<%= f.text_area :text, placeholder: 'text', style: 'width: 100%;height: 200px;' %>
</div>
<div>
<%= f.submit "SENT" %>
</div>
<% end %>

</div>
</div>